#tipModal.modal.fade(tabindex='-1', role='dialog', aria-labelledby='tipModalLabel', aria-hidden='true')
    .modal-dialog
        .modal-content
            .modal-header
                button.close(type='button', data-dismiss='modal')
                    span(aria-hidden='true') ×
                    span.sr-only Close
                h4#tipModalLabel.modal-title 等待数据加载...
            .modal-body Loading...
            .modal-footer
                button.btn.btn-default(type='button', data-dismiss='modal') 关闭
                button.btn.btn-primary(type='button') 确定
